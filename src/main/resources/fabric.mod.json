{
	"schemaVersion": 1,
	"id": "modbase",
	"version": "${version}",
	"name": "ModBase - Block",
	"description": "This mod base adds block examples to the game.",
	"authors": [
		"UbiquitousNull"
	],
	"license": "CC0-1.0",
	"icon": "assets/modbase/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"ubiquitousnull.modbase.ModBase"
		],
		"client": [
			"ubiquitousnull.modbase.ModBaseClient"
		],
		"fabric-datagen": [
			"ubiquitousnull.modbase.ModBaseDataGenerator"
		]
	},
	"mixins": [
		"modbase.mixins.json",
		{
			"config": "modbase.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": ""
	}
}
